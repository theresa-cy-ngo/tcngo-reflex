<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.mycompany.tcngo_reflex.StatisticsView"
    android:background="@color/background_material_dark">

    <TextView android:text="Statistics for Reaction Timer"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:textColor="#ac9fa3" />

    <TextView android:text=""
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textBlank"
        android:textColor="#ac9fa3" />

    <TextView android:text="Minimum"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textBlank"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textMin"
        android:textColor="#ac9fa3" />
    <ListView
        android:id="@+id/min10List"
        android:layout_width="fill_parent"
        android:layout_height="263dp"
        android:layout_below="@+id/textMin"
        android:scrollbars="vertical">

    </ListView>


</RelativeLayout>
